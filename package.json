{
  "name": "ezems-skin-default",
  "version": "0.1.0",
  "description": "The default skin for ezEMS",
  "keywords": [
    "cms",
    "ui",
    "skin"
  ],
  "homepage": "https://github.com/CaptainIRS/ezems-default-skin",
  "bugs": {
    "url": "https://github.com/CaptainIRS/ezems-default-skin/issues"
  },
  "license": "MIT",
  "author": {
    "name": "CaptainIRS",
    "url": "https://captainirs.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptainIRS/ezems-default-skin.git"
  },
  "main": "./dist/app.css",
  "files": [
