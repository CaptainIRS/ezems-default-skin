    "cssnano": "^5.1.12",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "prettier": "2.7.1",
    "tailwindcss": "^3.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
